{
    "name": "chinleung/laravel-converge",
    "description": "A PHP wrapper for Converge's api.",
    "keywords": [
        "chinleung",
        "converge",
        "elavon"
    ],
    "homepage": "https://github.com/chinleung/laravel-converge",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chin Leung",
            "email": "hello@chinleung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "ChinLeung\\Converge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChinLeung\\Converge\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChinLeung\\Converge\\ConvergeServiceProvider"
            ]
        }
    }
}
